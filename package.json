{
  "name": "adorable-accretion",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "dev:e2e": "astro dev --mode e2e",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "lint": "eslint \"src/**/*.{ts,vue,astro}\" --fix",
    "lint:check": "eslint \"src/**/*.{ts,vue,astro}\"",
    "format": "prettier \"src/**/*.{astro,ts,vue}\" --write",
    "format:check": "prettier \"src/**/*.{astro,ts,vue}\" --check",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:all": "npm run test:coverage && npm run test:e2e"
  },
  "dependencies": {
    "@astrojs/vue": "^5.0.13",
    "@primeuix/themes": "^1.1.1",
    "@primevue/forms": "^4.3.5",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.49.8",
    "@tailwindcss/vite": "^4.1.5",
    "@vee-validate/zod": "^4.15.1",
    "@vueuse/core": "^13.3.0",
    "astro": "^5.7.12",
    "date-fns": "^4.1.0",
    "dotenv": "^16.5.0",
    "pinia": "^3.0.2",
    "primeicons": "^7.0.0",
    "primevue": "^4.3.4",
    "tailwindcss": "^4.1.5",
    "tailwindcss-primeui": "^0.6.1",
    "vee-validate": "^4.15.1",
    "vue": "^3.5.13",
    "zod": "^3.25.63"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.8.0",
    "@playwright/test": "^1.49.1",
    "@typescript-eslint/parser": "^8.32.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitest/ui": "^3.2.2",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-vue": "^10.1.0",
    "faker": "^6.6.6",
    "globals": "^16.1.0",
    "jsdom": "^26.0.0",
    "msw": "^2.6.8",
    "prettier": "^3.5.3",
    "prettier-plugin-astro": "^0.14.1",
    "typescript-eslint": "^8.32.0",
    "vitest": "^3.2.2"
  }
